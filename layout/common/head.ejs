<meta charset="utf-8">
<title><%= page_title() %></title>
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/outdated-browser/1.1.5/outdatedbrowser.min.css">

<% if (has_config('head.open_graph')) { %>
    <%- open_graph({
        twitter_id: get_config('head.open_graph.twitter_id', ''),
        twitter_site: get_config('head.open_graph.twitter_site', ''),
        google_plus: get_config('head.open_graph.google_plus', ''),
        fb_admins: get_config('head.open_graph.fb_admins', ''),
        fb_app_id: get_config('head.open_graph.fb_app_id', '')
    }) %>
<% } %>

<% if (has_config('rss')) { %>
<link rel="alternative" href="<%- get_config('rss') %>" title="<%= get_config('title') %>" type="application/atom+xml">
<% } %>

<% if (has_config('favicon')) { %>
<link rel="icon" href="<%- get_config('favicon') %>">
<% } %>

<link rel="stylesheet" href="//fonts.googleapis.com/css?family=Ovo|Source+Code+Pro">
<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/bulma/0.6.2/css/bulma.min.css">

<% if (!(has_config('plugins.gallery') && get_config('plugins.gallery') === false)) { %>
<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/lightgallery/1.6.8/css/lightgallery.min.css">
<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/justifiedGallery/3.6.5/css/justifiedGallery.min.css">
<link rel="stylesheet" href="http://cdn.bootcss.com/font-awesome/4.6.3/css/font-awesome.css">

<% } %>

<%- css('css/style') %>
<% if (has_config('leancloud_visitors.enable')) { %>
    <script src="//cdn1.lncld.net/static/js/3.6.8/av-min.js"></script>
    <script>AV.initialize("<%- get_config('leancloud_visitors.app_id') %>", "<%- get_config('leancloud_visitors.app_key') %>");</script>
<% } %>
